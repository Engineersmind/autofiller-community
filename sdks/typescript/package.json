{
  "name": "@autofiller/sdk",
  "version": "0.1.0",
  "description": "Official TypeScript/JavaScript SDK for Autofiller Community Edition",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "autofiller",
    "document-extraction",
    "ocr",
    "pdf",
    "data-extraction",
    "invoice",
    "forms"
  ],
  "author": "Autofiller Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/autofiller-community.git",
    "directory": "sdks/typescript"
  },
  "homepage": "https://github.com/your-org/autofiller-community#readme",
  "bugs": {
    "url": "https://github.com/your-org/autofiller-community/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
